BUCKET %{NOTSPACE:ecs_bucket_name}
BUCKET_NAMESPACE %{NOTSPACE:ecs_namespace}
BUCKET_USERNAME %{NOTSPACE:ecs_bucket_username}
BUCKET_OPERATION BUCKET (CREATE|UPDATE|DELETE)
BUCKET_CREATED_MESSAGE has been created
BUCKET_UPDATE_MESSAGE has been updated
BUCKET_DELETE_MESSAGE has been updated
BUCKET_CREATED %{BUCKET_USERNAME} Bucket %{BUCKET} %{BUCKET_CREATED_MESSAGE:ecs_message}
BUCKET_UPDATED %{BUCKET_USERNAME} Bucket %{BUCKET} %{BUCKET_UPDATE_MESSAGE:ecs_message}
BUCKET_DELETED %{BUCKET_USERNAME} Bucket %{BUCKET} %{BUCKET_DELETE_MESSAGE:ecs_message}
BUCKET_QUOTA_SIZE %{NUMBER:ecs_quota_soft}/%{NUMBER:ecs_quota_hard}
BUCKET_QUOTA_UPDATE_MESSAGE quota has been updated with soft/hard size\(GB\):%{BUCKET_QUOTA_SIZE}
BUCKET_QUOTA_UPDATE %{BUCKET_USERNAME} Bucket %{BUCKET} %{BUCKET_QUOTA_UPDATE_MESSAGE}
BUCKET_METERING_BLK_EXCEEDED Metering (Notification|Block) size has been exceeded on
BUCKET_METERING %{BUCKET_METERING_BLK_EXCEEDED:ecs_quota_exceeded} %{BUCKET} bucket
ECS_ALERT_PREFIX ALERT ALERT
ECS_ALERT_TYPE %{GREEDYDATA}
ECS_ALERT_INFO %{DATA}%{BUCKET_NAMESPACE}\.%{BUCKET}
ECS_SYMPTOM_CODE %{NUMBER:ecs_symptom_code}
ECS_QUOTA_PREFIX Quota
ECS_QUOTA_INFO %{DATA:ecs_quota_info}
ECS_QUOTA_NAMESPACE Namespace %{NOTSPACE:ecs_quota_namespace}
ECS_QUOTA_BUCKET bucket %{NOTSPACE:ecs_quota_bucket}
ECS_LOGIN_PREFIX (LOCAL_USER DELETE|USER RETRIEVE|LOGIN UPDATE)
ECS_LOGIN_TOKEN urn:Token:%{NOTSPACE:ecs_login_token}
ECS_MGMT_USERNAME %{NOTSPACE:ecs_mgmt_user}
ECS_LOGOUT_MESSAGE User logged out token
ECS_MGMT_LOGOUT %{ECS_LOGIN_TOKEN} %{ECS_LOGOUT_MESSAGE:ecs_login_logout_message} %{NOTSPACE:ecs_mgmt_user}
ECS_CLIENT_IP %{IP:ecs_client_ip}
ECS_RETRIEVE_SECRET Retrieved secret key for object user
ECS_FAILED_LOGIN_MESSAGE1 User %{ECS_MGMT_USERNAME} failed to login.
ECS_FAILED_LOGIN_MESSAGE2 Access is denied due to invalid or expired credentials

# Aggregate all fields
ECS_BUCKET_LOG %{BUCKET_OPERATION:ecs_bucket_operation}%{SPACE}%{BUCKET_NAMESPACE:ecs_namespace}%{SPACE}(%{BUCKET_METERING:ecs_message}|%{BUCKET_CREATED}|%{BUCKET_UPDATED}|%{BUCKET_QUOTA_UPDATE})
ECS_ALERT_PARSE %{ECS_ALERT_PREFIX}%{SPACE}%{ECS_ALERT_TYPE:ecs_alert_type}%{SPACE}"%{ECS_ALERT_INFO:ecs_alert_info}"%{SPACE}with symptom code %{ECS_SYMPTOM_CODE} triggered
ECS_QUOTA_PARSE %{ECS_QUOTA_PREFIX} %{ECS_SYMPTOM_CODE} %{BUCKET_NAMESPACE} %{ECS_QUOTA_INFO}: (%{ECS_QUOTA_NAMESPACE}|%{ECS_QUOTA_BUCKET})

ECS_LOGOUT_LOG %{ECS_LOGIN_PREFIX:ecs_login_action}%{SPACE}%{ECS_LOGIN_TOKEN} %{ECS_LOGOUT_MESSAGE:ecs_login_logout_message} %{NOTSPACE:ecs_mgmt_user}
ECS_RETRIEVE_SECRET_LOG %{ECS_LOGIN_PREFIX:ecs_login_action}%{SPACE}%{BUCKET_NAMESPACE} %{IPORHOST:ecs_client_ip} %{ECS_MGMT_USERNAME} %{ECS_RETRIEVE_SECRET} %{BUCKET_USERNAME}
ECS_FAILED_LOGIN_LOG %{ECS_LOGIN_PREFIX:ecs_login_action}%{SPACE}%{IP:ecs_client_ip} %{NOTSPACE} %{ECS_FAILED_LOGIN_MESSAGE1:ecs_login_error}%{SPACE}%{ECS_FAILED_LOGIN_MESSAGE2:ecs_login_failed_login_reason}
ECS_LOG_PARSE (%{ECS_BUCKET_LOG}|%{ECS_ALERT_PARSE}|%{ECS_QUOTA_PARSE}|%{ECS_LOGOUT_LOG}|%{ECS_RETRIEVE_SECRET_LOG}|%{ECS_FAILED_LOGIN_LOG})